<launch>
  <include
    file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find kdarpa_world)/worlds/kdarpa.world"/>
  </include>
  <node
    name="tf_footprint_base"
    pkg="tf"
    type="static_transform_publisher"
    args="13.932 -18.075 -30.466 0 0 0 base_link base_footprint 40" />
  <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-file $(find kdarpa_world)/urdf/KDARPA.urdf -urdf -model kdarpa_world -x 53 -y 34 -z -16 -R 0 -P 0 -Y 0"
    output="screen" />
  <node
    name="fake_joint_calibration"
    pkg="rostopic"
    type="rostopic"
    args="pub /calibrated std_msgs/Bool true" />
</launch>
